{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "env": {
    "NEXT_PUBLIC_USE_MOCK_DATA": "true",
    "NEXT_PUBLIC_SITE_URL": "https://politician-finder.vercel.app",
    "NEXT_PUBLIC_SITE_NAME": "정치인 찾기 - PoliticianFinder",
    "NEXT_PUBLIC_APP_ENV": "production",
    "NEXT_PUBLIC_SUPABASE_URL": "https://dummy.supabase.co",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "dummy-key-for-mock-data",
    "SUPABASE_SERVICE_ROLE_KEY": "dummy-service-role",
    "UPSTASH_REDIS_REST_URL": "https://dummy-redis.upstash.io",
    "UPSTASH_REDIS_REST_TOKEN": "dummy-redis-token",
    "EMAIL_SERVICE_API_KEY": "dummy-email-key",
    "EMAIL_FROM": "noreply@politicianfinder.com",
    "NEXT_PUBLIC_GA_ID": "UA-DUMMY-1",
    "NEXT_PUBLIC_SENTRY_DSN": "https://dummy@sentry.io/000000",
    "NEXT_PUBLIC_ENABLE_BETA_FEATURES": "false",
    "NEXT_PUBLIC_ENABLE_MFA": "true",
    "NEXTAUTH_SECRET": "dummy-nextauth-secret-for-build-123456789",
    "NEXTAUTH_URL": "https://politician-finder.vercel.app",
    "NEXT_PUBLIC_CDN_URL": "https://cdn.example.com"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
