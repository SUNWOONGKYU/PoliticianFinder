{
  "name": "politician-frontend",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "eslint",
    "perf": "next build && next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathIgnorePatterns=e2e",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:chromium": "playwright test --project=chromium-desktop",
    "test:e2e:firefox": "playwright test --project=firefox-desktop",
    "test:e2e:mobile": "playwright test --project=mobile-chrome",
    "test:notifications": "playwright test notifications.spec.ts",
    "test:bookmarks": "playwright test bookmarks.spec.ts",
    "test:comments": "playwright test comments.spec.ts",
    "test:phase3": "playwright test notifications.spec.ts bookmarks.spec.ts comments.spec.ts",
    "test:critical": "playwright test critical-flow-*.spec.ts",
    "test:security": "playwright test security-owasp.spec.ts",
    "test:load": "k6 run k6/load-test-basic.js",
    "test:load:stress": "k6 run k6/load-test-stress.js",
    "test:load:spike": "k6 run k6/load-test-spike.js",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:phase4": "npm run test:unit && npm run test:critical && npm run test:security"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.39.3",
    "@upstash/ratelimit": "^1.0.3",
    "@upstash/redis": "^1.28.4",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "isomorphic-dompurify": "^2.12.0",
    "lucide-react": "^0.545.0",
    "zod": "^3.24.1",
    "next": "15.5.5",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.5",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
